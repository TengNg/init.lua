{
    "if": {
        "prefix": "if",
        "body": ["<% if ${1:truevalue} %>", "  $2", "<% end %>"],
        "description": "if .. end"
    },
    "else": {
        "prefix": "else",
        "body": ["<% else %>"],
        "description": "else"
    },
    "end": {
        "prefix": "end",
        "body": ["<% end %>"],
        "description": "end"
    },
    "render": {
        "prefix": ["re", "="],
        "body": ["<%= $1 %>"],
        "description": "render block pe"
    },
    "exec": {
        "prefix": ["ee", "%"],
        "body": ["<% $1 %>"],
        "description": "erb exec block"
    }
}
